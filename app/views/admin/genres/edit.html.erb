<div class="container">
  <div class="row">
    <div class="col-12">
      <h4>ジャンル編集</h4>
      <%= form_with model: @genre, url: admin_genre_path(@genre) do |f| %>
        <%= f.label :ジャンル名 %>
        <%= f.text_field :name %>
        <%= f.submit "変更を保存" %>
      <% end %>
    </div>
  </div>
</div>
